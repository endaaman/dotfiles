[include]
  path = ~/.gitconfig_local
[core]
  excludesfile = ~/.gitignore_global
  editor = $EDITOR -c \"set fenc=utf-8\"

[alias]
  a = add
  aa = add .
  ac = !git add . && git commit
  aca = !git add . && git commit --amend
  br = branch
  c = commit
  ca = commit --amend
  cm = commit -m
  co = checkout
  cp = cherry-pick
  d = diff
  f = fetch
  fe = fetch
  gr = grep
  i = rebase -i
  i2 = rebase -i @~2
  i3 = rebase -i @~3
  i4 = rebase -i @~4
  i5 = rebase -i @~5
  i6 = rebase -i @~6
  l = log
  l1 = log -n 10 --oneline --no-merges
  l1m = log -n 10 --oneline
  lp = log -p -1
  lpn = log -p -1 --name-only
  pl = pull
  ps = push -u
  rb = rebase
  rs = reset
  rsf = checkout HEAD --
  s = status -u
  st = stash

[color]
  ui = true
  diff = true
[ghq]
  root = ~/src
[http "https://gopkg.in"]
  followRedirects = true
