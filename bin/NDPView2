#!/usr/bin/bash

export WINEARCH=win64
if [ -z "${WINEPREFIX}" ]; then
  export WINEPREFIX="$HOME/Wines/ndp"
fi

if [ -n "$1" ]; then
  p=$(winepath -w "$1")
else
  P=""
fi

# zenity --info --title=title --text="$p" --no-markup
wine $WINEPREFIX/drive_c/Program\ Files/Hamamatsu/NDP.view\ 2/NDPView2.exe $p
